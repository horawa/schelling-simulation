#!/usr/bin/env python3
import sys
import os
import shutil

# T,v
results = [
	(0.25, 0.1),
	(0.25, 0.13),
	(0.25, 0.4),
	(0.375, 0.02),
	(0.375, 0.05),
	(0.375, 0.4),
	(0.625, 0.01),
	(0.625, 0.1),
]

output_path = "examples"
results_path = sys.argv[1]

for T, v in results:
	img_dir = os.path.join(results_path, "v_" + str(v), "T_" + str(T), "0")
	img = sorted(filter(lambda i: i[-4:] == ".png", os.listdir(img_dir)))[-1]
	img_path = os.path.join(img_dir, img)
	img_out = os.path.join(output_path, "T_"+str(T)+"_v_"+str(v)+".png")

	shutil.copy(img_path, img_out)
	print(img)
